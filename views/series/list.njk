{% extends '_skeletons/page.njk' %}

{% block content %}
<div class="section-title">
	<h2>{{ page.title }}</h2>
</div>
	<ul class="series">
		{% for serie in series %}
			<li class="list-item">
				<a class="title" href="/series/{{ serie.slug }}">{{ serie.title }}</a>
				<a class="publisher" href="/publisher/{{ serie.publisher.slug }}">{{ serie.publisher.name }}</a>
			</li>
		{% endfor %}
	</ul>
{% endblock %}
